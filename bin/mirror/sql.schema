CREATE TABLE `ftpmirrors` (
  `site` varchar(255) NOT NULL default '',
  `admin` varchar(255) NOT NULL default '',
  `admin_email` varchar(255) NOT NULL default '',
  `country_iso` varchar(5) NOT NULL default '',
  `sequence` int(11) default '0',
  `other_details` varchar(255) NOT NULL default '',
  `updated` varchar(255) NOT NULL default '',
  `live` enum('false','true') NOT NULL default 'true',
  `city` varchar(255) NOT NULL default '',
  `tstamp` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  PRIMARY KEY  (`site`)
);

CREATE TABLE `wwwmirrors` (
  `site` varchar(255) NOT NULL default '',
  `admin` varchar(255) NOT NULL default '',
  `admin_email` varchar(255) NOT NULL default '',
  `country_iso` varchar(5) NOT NULL default '',
  `sequence` int(11) default '0',
  `other_details` varchar(255) NOT NULL default '',
  `updated` varchar(255) NOT NULL default '',
  `live` enum('false','true') NOT NULL default 'true',
  `city` varchar(255) NOT NULL default '',
  `tstamp` timestamp NOT NULL default CURRENT_TIMESTAMP on update CURRENT_TIMESTAMP,
  PRIMARY KEY  (`site`)
);
